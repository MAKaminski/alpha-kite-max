# Streaming Configuration - Real-time streaming (every second)
STREAM_TICKER=QQQ
BATCH_SIZE=1          # Write every tick immediately (set higher for batching)
BATCH_INTERVAL=1      # Flush every second (set higher to reduce DB writes)

# Schwab API Credentials
SCHWAB_APP_KEY=your_schwab_app_key
SCHWAB_APP_SECRET=your_schwab_app_secret
SCHWAB_REDIRECT_URI=https://localhost:8080

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Optional: Skip market hours check
SKIP_MARKET_HOURS=false

